								</Column>
								<Column id="column1">
									<header>
										<Label text="{i18n>genere}" id="label1"/>
									</header>
								</Column>
								<Column id="column2">
									<header>
										<Label text="{i18n>anno}" id="label2"/>
									</header>
								</Column>
								<Column id="column3">
									<header>
										<Label text="{i18n>regista}" id="label3"/>
									</header>
								</Column>
							</columns>
						</Table>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>