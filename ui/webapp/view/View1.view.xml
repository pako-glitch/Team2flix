<mvc:View controllerName="Team2flix.ui.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>master}">
					<content>
						<Table noDataText="No Data" id="table0" items="{modello>/POHeader}">
							<items>
								<ColumnListItem type="Active" id="item0">
									<cells>
										<Text text="{modello>titoloserie}" id="text2"/>
										<Text text="{modello>genere}" id="text3"/>
										<Text text="{modello>anno}" id="text4"/>
										<Text text="{modello>regista}" id="text5"/></cells>
