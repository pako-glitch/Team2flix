{
  "welcomeFile": "/ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/modello/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "modello",
      "csrfProtection": false
    },
    {
      "source": "^/ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "(.*)(.xsjs)",
      "destination": "core-xsjs-backend",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(.*)(.xsodata)",
      "destination": "core-xsjs-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/flix_dest/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "flix_dest",
      "csrfProtection": false
    }
  ]
}